{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "api/daily-championship.ts": {
      "maxDuration": 300
    }
  },
  "rewrites": [
    {
      "source": "/webhook",
      "destination": "api/webhook.ts"
    }
  ],
  "crons": [
    {
      "path": "/api/daily-championship",
      "schedule": "50 17 * * *"
    }
  ]
}